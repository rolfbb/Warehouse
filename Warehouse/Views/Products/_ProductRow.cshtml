@model Warehouse.Models.Product

<tr>
    <td>            @Html.DisplayFor(product => product.Name)        </td>
    <td>            @Html.DisplayFor(product => product.Price)        </td>
    <td>            @Html.DisplayFor(product => product.Quantity)        </td>
    <td>            @Html.DisplayFor(product => product.Category)        </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { Id = Model.Id, Weather="fine" }) |
        @Html.ActionLink("Details", "Details", new { id = Model.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = Model.Id })
    </td>
</tr>
